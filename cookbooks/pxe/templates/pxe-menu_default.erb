default menu.c32
prompt 0
timeout 30
menu title ########## PXE Boot Menu ##########
label 1
menu label CentOS 7 Installation with DEFAULT settings
kernel centos7_x64/images/pxeboot/vmlinuz
append initrd=centos7_x64/images/pxeboot/initrd.img method=http://<%= @pxe_ip %>/centos7_x64 devfs=nomount ks=http://<%= @pxe_ip %>/ks/kickstart-default.ks
